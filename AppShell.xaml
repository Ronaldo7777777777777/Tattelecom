<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Tattelecom.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Tattelecom"
    Shell.FlyoutBehavior="Disabled"
    Title="Tattelecom">

    <ShellContent Title="Home"  ContentTemplate="{DataTemplate local:LoginPage}"  Route="LoginPage" />

    <ShellContent  Title="Authorization"  ContentTemplate="{DataTemplate local:MainPage}" Route="MainPage" />

    <ShellContent  Title="Registration"  ContentTemplate="{DataTemplate local:RegistrationPage}" Route="RegistrationPage" />

    <ShellContent  Title="AuthorizationSotrudnik"  ContentTemplate="{DataTemplate local:LoginPageSotrudnik}" Route="LoginPageSotrudnik" />

    <ShellContent  Title="CreateTicketPage"  ContentTemplate="{DataTemplate local:CreateTicketPage}" Route="CreateTicketPage" />




</Shell>
